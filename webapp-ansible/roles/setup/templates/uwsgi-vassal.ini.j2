
[uwsgi]
socket = {{ project_dir }}/uwsgi.sock
chmod-socket = 775
chdir = {{ project_dir }}
master = true
binary-path = {{ project_dir }}/bin/uwsgi
virtualenv = {{ virtualenv_dir }}
module = {{ uwsgi_module }}
uid = http
gid = http
processes = {{ uwsgi_processes }}
threads = {{ uwsgi_threads }}
plugins = {{ uwsgi_plugins }}
logger = file:{{ project_dir }}/uwsgi.log
{% for env in uwsgi_env %}
env = {{ env }}
{% endfor %}